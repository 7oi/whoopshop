@model IEnumerable<Bordspil.Models.UserProfile>
@{
    ViewBag.Title = "UserList";
}

<h2>User List</h2>

<div>
    @foreach (var item in Model)
    {
        <div class="userlist-user-wrap">
            <h3>@item.UserName</h3>
            <img class="userlist-prof-pic" src="@item.ProfilePicUrl" />
            <p>@item.Points</p>
        </div>
    }

</div>
<div class="hreinsarinn">

</div>
