@model IEnumerable<Bordspil.Models.UserProfile>
@{
    ViewBag.Title = "UserList";
}

<h2>User List</h2>

<div>
    @foreach (var item in Model)
    {
        <h3>@item.UserName</h3>
        <img id="userlist-prof-pic" src="@item.ProfilePicUrl" alt="Profile Pic" />
        <p>@item.Points</p>
    }

</div>